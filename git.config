[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
	hideDotFiles = dotGitOnly
[remote "origin"]
	url = git@github.com:BryanApellanes/BamContent.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
	remote = origin
	merge = refs/heads/master
[branch "BamContent"]
	remote = origin
	merge = refs/heads/BamContent
[submodule "common/bamvvm"]
	url = git@github.com:BryanApellanes/bamvvm.git
[submodule "Apps"]
	url = git@github.com:BryanApellanes/Apps.git
[remote "bitbucket"]
	url = git@bitbucket.org:threeheadz/bamcontent.git
	fetch = +refs/heads/*:refs/remotes/bitbucket/*
[remote "gitlab"]
	url = git@gitlab.com:bryan.apellanes/BamContent.git
	fetch = +refs/heads/*:refs/remotes/gitlab/*
[remote "github"]
	url = git@github.com:BryanApellanes/BamContent.git
	fetch = +refs/heads/*:refs/remotes/github/*
[remote "all"]
	url = git@github.com:BryanApellanes/BamContent.git
	url = git@bitbucket.org:threeheadz/bamcontent.git
	url = git@gitlab.com:bryan.apellanes/BamContent.git
