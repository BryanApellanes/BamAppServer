apiVersion: apps/v1
kind: Deployment
metadata:
  name: {{AppName}}-deployment
  labels:
    app: {{AppName}}
spec:
  replicas: {{ReplicaCount}}
  selector:
    matchLabels:
      app: {{AppName}}
  template:
    metadata:
      labels:
        app: {{AppName}}
    spec:
      containers:
      - name: {{AppName}}
        image: {{AppName}}:{{Tag}}
        resources:
          limits:
            cpu: 100m
            memory: "2Gi"
        ports:
        - containerPort: 80
      