{
  "GenerateDao": true,
  "ServiceSearchPattern": "*Services.dll,*Proxyables.dll",
  "DaoSearchPattern": "*Dao.dll",
  "ProcessModes": [
    "Dev",
    "Test",
    "Prod"
  ],
  "ContentRoot": "/opt/bam/content",
  "DaoConfigs": [
    {
      "ConnectionName": "DaoConf",
      "RegistrarCaller": "Bam.Net.Data.SQLiteRegistrarCaller, bam.net.core, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"
    }
  ],
  "ProxyAliases": [],
  "LoggerPaths": [
    "."
  ],
  "LoggerSearchPattern": "*Logging.dll",
  "MainLoggerName": "ConsoleLogger",
  "ServerEventListenerSearchPath": "/bam/content/server-listeners,/bam/content/server-listeners-temp",
  "ServerEventListenerAssemblySearchPattern": "*ServerListeners.dll,*ServerEventListeners.dll",
  "MaxThreads": 50,
  "SchemaInitializers": [
    {
      "SchemaContext": "Bam.Net.UserAccounts.Data.UserAccountsContext, Bam.Net.UserAccounts, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null",
      "RegistrarCaller": "Bam.Net.Data.SQLiteRegistrarCaller, Bam.Net.Data.SQLite, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"
    }
  ]
}