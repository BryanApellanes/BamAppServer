{
  "Name": "bam",
  "DisplayName": "Bam",
  "ProcessMode": "Dev",
  "ServerConf": {
    "ServerKind": "Bam",
    "Command": "bamweb",
    "Arguments": [
      "/S",
      "/content:/opt/bam/content",
      "/verbose"
    ]
  },
  "Bindings": [
    {
      "HostName": "bam",
      "Port": 8080,
      "Ssl": false
    }
  ],
  "AppSettings": [],
  "DefaultLayout": "basic",
  "DefaultPage": "start",
  "HtmlDir": "pages",
  "GenerateDao": true,
  "CheckDaoHashes": false,
  "RenderLayoutBody": true,
  "ServiceSearchPattern": [
    "*Services.dll",
    "*Proxyables.dll"
  ],
  "ServiceReferences": [],
  "ServiceTypeNames": [
    "Bam.Net.ServiceProxy.Echo, bam.net.core, Version=1.0.1.0, Culture=neutral, PublicKeyToken=null",
    "Bam.Net.ServiceProxy.EncryptedEcho, bam.net.core, Version=1.0.1.0, Culture=neutral, PublicKeyToken=null"
  ],
  "SchemaInitializers": [],
  "AppInitializer": null,
  "AppInitializerAssemblyPath": null,
  "UserManagerConfig": {
    "ApplicationName": "UNKNOWN-APPLICATION",
    "ApplicationNameResolverType": "Bam.Net.Configuration.DefaultConfigurationApplicationNameProvider, bam.net.core, Version=1.0.1.0, Culture=neutral, PublicKeyToken=null",
    "EmailComposerType": "Bam.Net.Messaging.NamedFormatEmailComposer, bam.net.core, Version=1.0.1.0, Culture=neutral, PublicKeyToken=null",
    "EmailTemplateDirectoryPath": "/opt/bam/data/Dev/AppData/bamweb/EmailTemplates",
    "SmtpSettingsVaultPath": "/opt/bam/data/Dev/AppData/bamweb/SmtpSettings.vault.sqlite",
    "SuppressMessages": false
  }
}